    "axios": "^1.6.2",
    "date-fns": "^3.0.5",
    "filesize": "^10.1.0",
    "oidc-client-ts": "^2.4.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.44.0",
    "qrcode.vue": "^3.4.1",
    "vee-validate": "^4.12.3",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5",
    "yup": "^1.3.3"
