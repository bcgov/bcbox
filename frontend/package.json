    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^8.9.0",
    "jsdom": "^21.1.1",
    "rimraf": "^4.1.2",
    "sass": "^1.58.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  }
